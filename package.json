    "json-server": "0.17.4",
    "json-server-reset": "1.6.0",
    "jsonc-eslint-parser": "2.1.0",
    "lcov-result-merger": "5.0.0",
    "lint-staged": "15.2.0",
    "ngrx-store-freeze": "0.2.4",
    "nx": "18.1.1",
    "prettier": "3.1.0",
    "purgecss": "5.0.0",
    "stylelint": "15.10.1",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-order": "6.0.3",
