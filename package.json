{
  "name": "nx-resume-client",
  "version": "14.0.0-alpha.1",
  "author": "Scott Robertson (https://github.com/sc0tt5)",
  "license": "SEE LICENSE",
  "private": true,
  "engines": {
    "node": "^16.14.0 || ^18.10.0"
  },
  "scripts": {
    "ng": "nx",
    "start": "npm run dev:ssr",
    "build": "ng build",
    "test": "nx run-many --target=test --all --codeCoverage && npm run test:results && npm run test:summary",
    "test:results": "istanbul-merge --out tmp/coverage/coverage-final.json tmp/coverage/**/*coverage-final.json",
    "test:summary": "ts-node tools/scripts/merge-coverage.ts",
    "lint": "npm run lint:styles && nx run-many --target=lint --all",
    "lint:styles": "stylelint \"{apps,libs}/**/*.scss\"",
    "compodoc": "compodoc --disableCoverage -p tsconfig.doc.json --unitTestCoverage tmp/coverage/coverage-summary.json -a apps/resume-client/src/assets -d tmp/documentation -s -o",
    "compodoc:readme": "ts-node tools/scripts/transform-doc-index.ts",
    "lighthouse": "lhci autorun",
    "analyze:bundles": "ng run resume-client:analyze --diffLoading=false",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "serve:resume-server": "ng serve resume-server",
    "serve:database": "json-server --watch db.json --port 8800 --middlewares ./node_modules/json-server-reset",
    "dev:ssr": "npm run prebuild && concurrently --names 'JSONDB,NESTJS' --prefix-colors 'blue,yellow,' --kill-others \"npm:serve:database\" \"ng run resume-client:serve-ssr:local --open\"",
    "build:ssr": "ng build --prod --skipNxCache && ng run resume-client:server:production",
    "serve:ssr": "concurrently --names 'JSONDB,SERVER' --prefix-colors 'blue,yellow,' --kill-others \"npm:serve:database\" \"node dist/resume-client/server/main.js --prod\"",
    "prod:ssr": "npm run build:ssr && npm run serve:ssr",
    "prebuild:ssr": "npm run prebuild",
    "postbuild:ssr": "ts-node tools/scripts/postbuild.ts",
    "prebuild": "ts-node tools/scripts/prebuild.ts",
    "prepare": "husky install",
    "prerender": "ng run resume-client:prerender",
    "postinstall": "node ./tools/scripts/decorate-angular-cli.js",
    "workspace-generator": "nx workspace-generator"
  },
  "dependencies": {
    "@angular/animations": "16.0.3",
